<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sxau.cyschool.pojo.AdminEntity" table="admin" schema="cyschool">
        <id name="aId" column="a_id"/>
        <property name="aName" column="a_name"/>
        <property name="aPhone" column="a_phone"/>
        <property name="aGrade" column="a_grade"/>
        <property name="aPassword" column="a_password"/>
    </class>
</hibernate-mapping>