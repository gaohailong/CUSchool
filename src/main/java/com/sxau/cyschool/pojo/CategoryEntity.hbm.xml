<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sxau.cyschool.pojo.CategoryEntity" table="category" schema="cyschool">
        <id name="cId" column="c_id"/>
        <property name="cContent" column="c_content"/>
        <property name="cNumber" column="c_number"/>
    </class>
</hibernate-mapping>