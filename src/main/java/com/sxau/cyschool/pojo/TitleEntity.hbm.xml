<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sxau.cyschool.pojo.TitleEntity" table="title" schema="cyschool">
        <id name="tId" column="t_id"/>
        <property name="tName" column="t_name"/>
        <property name="tContent" column="t_content"/>
        <property name="tRead" column="t_read"/>
        <property name="tDate" column="t_date"/>
        <property name="tCategory" column="t_category"/>
        <property name="tLike" column="t_like"/>
        <property name="tSort" column="t_sort"/>
    </class>
</hibernate-mapping>